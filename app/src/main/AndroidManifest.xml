<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jwake.myapplication" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SplashScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HomeScreen"
            android:label="@string/title_activity_home_screen" >
        </activity>
        <activity
            android:name=".IntroScreen"
            android:label="@string/title_activity_intro_screen" >
        </activity>
        <activity
            android:name=".MakeAccount"
            android:label="@string/title_activity_make_account" >
        </activity>
        <activity
            android:name=".TaskInstructions"
            android:label="@string/title_activity_task_instructions" >
        </activity>
    </application>

</manifest>
